div(layout='column' ng-form)
  div
    md-input-container
      label(for="login-username") User Name
      input(id="login-username" type=text name='username' ng-model='login.username')
  div
    md-input-container
      label(for="login-password") User Password
      input(id="login-password" type=text name='password' ng-model='login.password')
  div
    md-button(ng-click='login.login()') Login
